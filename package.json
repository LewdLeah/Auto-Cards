  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@eslint/js": "^9.27.0",
    "ai.dungeon": "workspace:^",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LewdLeah/Auto-Cards.git"
  },
  "private": true,
  "keywords": [
    "autocards",
    "aidungeon",
    "aid"
