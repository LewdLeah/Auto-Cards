{
  "name": "auto-cards",
  "description": "Auto-Cards is an AI Dungeon script ",
  "author": "LewdLeah & contributors",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/LewdLeah/Auto-Cards",
  "bugs": {
    "url": "https://github.com/LewdLeah/Auto-Cards/issues"
  },
  "type": "module",
  "scripts": {
    "run:Dev": "dotenvx run --env AID_ENV=development -f .env --quiet -- node ./utils/ai.dungeon/index.js",
    "run:Prod": "dotenvx run --env AID_ENV=production -f .env --quiet -- node ./utils/ai.dungeon/index.js"
